{
  "name": "@uncover/js-utils-test",
  "version": "0.0.1",
  "description": "javascript test utils",
  "author": "aSh <ash.uncover@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ash-uncover/js-utils-test"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\" \"src/**/*.jsx\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test-coverage && npm run lint",
    "preversion": "npm run lint",
    "test": "jest",
    "test-coverage": "jest --collectCoverage",
    "version": "npm run format"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/jest": "latest",
    "@types/node": "latest",
    "jest": "latest",
    "prettier": "latest",
    "ts-jest": "latest",
    "tslint": "latest",
    "tslint-config-prettier": "latest",
    "typescript": "latest"
  },
  "dependencies": {}
}
